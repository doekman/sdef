<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionary SYSTEM "file://localhost/System/Library/DTDs/sdef.dtd">
<dictionary>
	<suite name="Terminology Suite" code="ascr" description="Terms that are for backward compatibility only">
		<class name="Exchange account" code="ExAc" description="An Exchange account" plural="Exchange accounts">
			<property name="free busy server" code="ExFr" type="text" description="the Exchange free/busy server"/>
			<property name="Exchange server" code="ExSv" type="text" description="the address of the Exchange server"/>
			<property name="DAV requires SSL" code="vSSL" type="boolean" description="true if an SSL connection is needed for DAV access"/>
			<property name="DAV port" code="DvPt" type="integer" description="the port to use when connecting to the Exchange server via DAV"/>
			<property name="LDAP server" code="ldpS" type="text" description="the address of the LDAP server"/>
			<property name="requires authentication" code="rqAt" type="boolean" description="true if the LDAP server requires a user name and password"/>
			<property name="LDAP requires SSL" code="LDPs" type="boolean" description="true if an SSL connection is needed for the LDAP server"/>
			<property name="LDAP port" code="LDPp" type="integer" description="the port to use when connecting to the LDAP server"/>
		</class>
		<class name="application" code="capp" description="An application program" plural="applications">
			<property name="in box folder" code="pInB" type="specifier" access="r" description="to the inbox"/>
			<property name="out box folder" code="pOtB" type="specifier" access="r" description="to the outbox"/>
			<property name="sent mail folder" code="pStM" type="specifier" access="r" description="to the sent mail folder"/>
			<property name="deleted mail folder" code="pDlM" type="specifier" access="r" description="to the deleted mail folder"/>
		</class>
		<enumeration name="xtyp" code="xtyp">
			<enumerator name="all" code="all " description="all"/>
			<enumerator name="categoryType" code="xcat" description="categories"/>
			<enumerator name="projectType" code="xprj" description="projects"/>
		</enumeration>
		<enumeration name="Xitm" code="Xitm">
			<enumerator name="mailItems" code="Xmal" description="mail"/>
			<enumerator name="contactItems" code="Xcon" description="contacts"/>
			<enumerator name="eventItems" code="Xevt" description="calendar events"/>
			<enumerator name="tasksItems" code="Xtsk" description="tasks"/>
			<enumerator name="noteItems" code="Xnot" description="notes"/>
			<enumerator name="fileItems" code="Xfil" description="files"/>
			<enumerator name="allItems" code="Xall" description="all"/>
		</enumeration>
		<enumeration name="xfmt" code="xfmt">
			<enumerator name="archive" code="rge " description="archive"/>
			<enumerator name="contacts" code="cnts" description="tab delimited contacts"/>
		</enumeration>
		<enumeration name="cnfl" code="cnfl">
			<enumerator name="stop" code="stop" description="stop importing if there are conflicts"/>
			<enumerator name="skip" code="skip" description="do not import conflicting records"/>
			<enumerator name="replace" code="rplc" description="overwrite records from the archive"/>
		</enumeration>
	</suite>
	<suite name="Standard Suite" code="CoRe" description="Common terms for most applications">
		<command name="close" code="coreclos" description="Close an object">
			<direct-parameter type="specifier" description="the object to close"/>
			<parameter name="saving" code="savo" type="savo" optional="yes" description="specifies whether changes should be saved before closing"/>
			<parameter name="saving in" code="kfil" type="alias" optional="yes" description="the file in which to save the object"/>
		</command>
		<command name="count" code="corecnte" description="Return the number of elements of a particular class within an object">
			<direct-parameter type="specifier" description="the object whose elements are to be counted"/>
			<parameter name="each" code="kocl" type="type" description="the class of the elements to be counted."/>
			<result type="integer" description="the number of elements"/>
		</command>
		<command name="data size" code="coredsiz" description="Return the size, in bytes, of an object">
			<direct-parameter type="specifier" description="the object whose data size is to be returned"/>
			<parameter name="as" code="rtyp" type="type" optional="yes" description="the data type for which the size is calculated"/>
			<result type="integer" description="the size of the object in bytes"/>
		</command>
		<command name="delete" code="coredelo" description="Delete an element from an object">
			<direct-parameter type="specifier" description="the element to delete"/>
		</command>
		<command name="duplicate" code="coreclon" description="Duplicate object(s)">
			<direct-parameter type="specifier" description="the object(s) to duplicate"/>
			<parameter name="to" code="insh" type="location specifier" optional="yes" description="the new location for the object(s)"/>
			<result type="specifier" description="to the duplicated object(s)"/>
		</command>
		<command name="exists" code="coredoex" description="Verify if an object exists">
			<direct-parameter type="specifier" description="the object in question"/>
			<result type="boolean" description="true if it exists, false if not"/>
		</command>
		<command name="get" code="coregetd" description="Get the data for an object">
			<direct-parameter type="specifier" description="the object whose data is to be returned"/>
			<parameter name="as" code="rtyp" description="the desired types for the data, in order of preference">
				<type type="type" list="yes"/>
			</parameter>
			<result type="any" description="the data from the object"/>
		</command>
		<command name="make" code="corecrel" description="Make a new element">
			<parameter name="new" code="kocl" type="type" description="the class of the new element."/>
			<parameter name="at" code="insh" type="any" description="the location at which to insert the element"/>
			<parameter name="with data" code="data" type="any" optional="yes" description="the initial data for the element"/>
			<parameter name="with properties" code="prdt" type="record" optional="yes" description="the initial values for the properties of the element"/>
			<result type="specifier" description="to the new object(s)"/>
		</command>
		<command name="move" code="coremove" description="Move object(s) to a new location">
			<direct-parameter type="specifier" description="the object(s) to move"/>
			<parameter name="to" code="insh" type="location specifier" description="the new location for the object(s)"/>
			<result type="specifier" description="to the object(s) after they have been moved"/>
		</command>
		<command name="open" code="aevtodoc" description="Open the specified object(s)">
			<direct-parameter type="specifier" description="the object(s) to open"/>
		</command>
		<command name="print" code="aevtpdoc" description="Print the specified object(s)">
			<direct-parameter description="the object(s) to print">
				<type type="specifier" list="yes"/>
			</direct-parameter>
			<parameter name="with properties" code="prdt" type="print settings" optional="yes" description="the print settings"/>
			<parameter name="print dialog" code="pdlg" type="boolean" optional="yes" description="Should the application show the print dialog?"/>
		</command>
		<command name="quit" code="aevtquit" description="Quit an application program">
			<parameter name="saving" code="savo" type="savo" optional="yes" description="Specifies whether or not to save currently open documents"/>
		</command>
		<command name="save" code="coresave" description="Save an object">
			<direct-parameter type="specifier" description="the object to save"/>
			<parameter name="in" code="kfil" type="text" optional="yes" description="the file in which to save the object"/>
			<parameter name="as" code="fltp" type="type" optional="yes" description="the file type of the document in which to save the data"/>
		</command>
		<command name="set" code="coresetd" description="Set an object's data">
			<direct-parameter type="specifier" description="the object to change"/>
			<parameter name="to" code="data" type="any" description="the new value"/>
		</command>
		<class name="print settings" code="pset">
			<property name="copies" code="lwcp" type="integer" access="r" description="the number of copies of a document to be printed "/>
			<property name="collating" code="lwcl" type="boolean" access="r" description="Should printed copies be collated?"/>
			<property name="starting page" code="lwfp" type="integer" access="r" description="the first page of the document to be printed"/>
			<property name="ending page" code="lwlp" type="integer" access="r" description="the last page of the document to be printed"/>
			<property name="pages across" code="lwla" type="integer" access="r" description="number of logical pages laid across a physical page"/>
			<property name="pages down" code="lwld" type="integer" access="r" description="number of logical pages laid out down a physical page"/>
			<property name="requested print time" code="lwqt" type="date" access="r" description="the time at which the desktop printer should print the document..."/>
			<property name="error handling" code="lweh" type="enum" access="r" description="how errors are handled"/>
			<property name="fax number" code="faxn" type="text" access="r" description="for fax number"/>
			<property name="target printer" code="trpr" type="text" access="r" description="for target printer"/>
		</class>
		<class-extension extends="application" description="An application program">
			<element type="window"/>
			<element type="POP account"/>
			<element type="IMAP account"/>
			<element type="Hotmail account"/>
			<element type="Exchange account"/>
			<element type="LDAP server"/>
			<element type="address book"/>
			<element type="folder"/>
			<element type="address"/>
			<element type="contact"/>
			<element type="group"/>
			<element type="news server"/>
			<element type="news group"/>
			<element type="calendar"/>
			<element type="event"/>
			<element type="schedule"/>
			<element type="signature"/>
			<element type="mailing list"/>
			<element type="category"/>
			<element type="project"/>
			<element type="custom view"/>
			<element type="multitype custom view"/>
			<element type="mail custom view"/>
			<element type="contacts custom view"/>
			<element type="calendar custom view"/>
			<element type="tasks custom view"/>
			<element type="notes custom view"/>
			<element type="task"/>
			<element type="note"/>
			<property name="frontmost" code="pisf" type="boolean" access="r" description="Is this the frontmost application?"/>
			<property name="name" code="pnam" type="text" access="r" description="The name of the application."/>
			<property name="selection" code="pusl" type="any" description="The selection visible to the user."/>
			<property name="version" code="vers" type="version" access="r" description="The version of the application."/>
			<property name="main window" code="mnWn" type="specifier" access="r" description="to the main window.  open main window will open the main window."/>
			<property name="progress window" code="prWn" type="specifier" access="r" description="to the progress window.  open progress window will open the progress window."/>
			<property name="connection in progress" code="pCnP" type="boolean" access="r" description="Are there any network connections in progress?"/>
			<property name="working offline" code="wkOf" type="boolean" description="Is Entourage working offline (not connected to the internet)?"/>
			<property name="default mail account" code="dfAc" type="specifier" description="to the default mail account"/>
			<property name="default news server" code="dfNS" type="specifier" description="to the default news server"/>
			<property name="default LDAP server" code="dfLA" type="specifier" description="to the default LDAP server"/>
			<property name="me contact" code="meCn" type="specifier" description="to the contact that represents the user"/>
			<property name="inbox folder" code="pInB" type="specifier" access="r" description="to the inbox"/>
			<property name="outbox folder" code="pOtB" type="specifier" access="r" description="to the outbox"/>
			<property name="sent items folder" code="pStM" type="specifier" access="r" description="to the sent items folder"/>
			<property name="deleted items folder" code="pDlM" type="specifier" access="r" description="to the deleted items folder"/>
			<property name="drafts folder" code="pDrF" type="specifier" access="r" description="to the drafts folder"/>
			<property name="current messages" code="CMgs" access="r" description="the current messaged depending on context, including message currently being filtered">
				<type type="specifier" list="yes"/>
			</property>
			<property name="current identity" code="CIdn" type="specifier" description="the current identity.  Set to change identities."/>
			<property name="path to MUD" code="PMud" type="alias" access="r" description="alias to the Microsoft User Data folder"/>
			<property name="localized language" code="LLng" type="integer" access="r" description="the Windows language ID for the locale that Entourage is using."/>
		</class-extension>
		<class name="window" code="cwin" description="A window" plural="windows">
			<property name="class" code="pcls" type="type" access="r" description="the type of window"/>
			<property name="bounds" code="pbnd" type="rectangle" description="the boundary rectangle for the window"/>
			<property name="closeable" code="hclb" type="boolean" access="r" description="does the window have a close box?"/>
			<property name="titled" code="ptit" type="boolean" access="r" description="does the window have a title bar?"/>
			<property name="index" code="pidx" type="integer" description="the index of the window (frontmost is 1, etc.)"/>
			<property name="floating" code="isfl" type="boolean" access="r" description="does the window float?"/>
			<property name="modal" code="pmod" type="boolean" access="r" description="is the window modal?"/>
			<property name="resizable" code="prsz" type="boolean" access="r" description="is the window resizable?"/>
			<property name="zoomable" code="iszm" type="boolean" access="r" description="is the window zoomable?"/>
			<property name="zoomed" code="pzum" type="boolean" description="is the window zoomed?"/>
			<property name="name" code="pnam" type="text" access="r" description="the title of the window"/>
			<property name="visible" code="pvis" type="boolean" access="r" description="is the window visible?"/>
			<property name="position" code="ppos" type="point" access="r" description="upper left coordinates of window"/>
			<property name="displayed message" code="pMsg" type="specifier" access="r" description="to the message being displayed, if there is one"/>
			<property name="displayed feature" code="pDsF" type="specifier" description="to the feature (folder, server, etc) being displayed, if there is one"/>
			<property name="content" code="ctnt" type="text" description="body of displayed message, if there is one"/>
		</class>
		<enumeration name="savo" code="savo">
			<enumerator name="yes" code="yes " description="Save objects now"/>
			<enumerator name="no" code="no  " description="Do not save objects"/>
			<enumerator name="ask" code="ask " description="Ask the user whether to save"/>
		</enumeration>
		<enumeration name="kfrm" code="kfrm">
			<enumerator name="index" code="indx" description="keyform designating indexed access"/>
			<enumerator name="named" code="name" description="keyform designating named access"/>
			<enumerator name="id" code="ID  " description="keyform designating access by unique identifier"/>
		</enumeration>
		<enumeration name="enum" code="enum">
			<enumerator name="standard" code="lwst" description="Standard PostScript error handling  "/>
			<enumerator name="detailed" code="lwdt" description="print a detailed report of PostScript errors"/>
		</enumeration>
	</suite>
	<suite name="Entourage Mail and News Suite" code="MSOE" description="Suite for accessing Entourage's mail and news features">
		<command name="connect to" code="MSOEcntP" description="Connect to account (POP, IMAP, or news)">
			<direct-parameter description="the account(s) to connect to">
				<type type="specifier" list="yes"/>
			</direct-parameter>
		</command>
		<command name="send" code="mailsend" description="Send e-mail messages">
			<direct-parameter optional="yes" description="the message(s) to send, or none to send all queued messages">
				<type type="message" list="yes"/>
			</direct-parameter>
			<parameter name="sending later" code="SnLt" type="boolean" optional="yes" description="Should the message just be queued to be sent later (defaults to false)?"/>
		</command>
		<command name="refresh" code="MSOEreFr" description="Refresh an object">
			<direct-parameter type="specifier" description="the object(s) to refresh"/>
		</command>
		<command name="purge" code="MSOEPFld" description="purge a folder">
			<direct-parameter type="specifier" description="the folder to purge (not relevant for all types of mail)"/>
		</command>
		<command name="execute" code="MSOEExec" description="Execute a schedule">
			<direct-parameter type="specifier" description="the schedule to run"/>
		</command>
		<command name="burst" code="MSOEBrst" description="Burst a digest message into constituent messages">
			<direct-parameter type="message" description="the message to burst"/>
			<parameter name="to" code="insh" type="location specifier" optional="yes" description="the folder to burst into (defaults to the same folder as the message)"/>
			<result description="to the bursted messages">
				<type type="specifier" list="yes"/>
			</result>
		</command>
		<command name="download" code="MSOEDwnd" description="Download an IMAP, Windows Live Hotmail, or News message that's on the server">
			<direct-parameter description="the message to download">
				<type type="message" list="yes"/>
			</direct-parameter>
		</command>
		<command name="forward" code="MSOEmFwd" description="Forward a message">
			<direct-parameter type="message" description="the message to forward"/>
			<parameter name="to" code="insh" type="text" optional="yes" description="recipients of new message (as text)"/>
			<parameter name="opening window" code="OpWn" type="boolean" optional="yes" description="Should it create a window or a message? (Default is to create window)"/>
			<parameter name="html text" code="AsHT" type="boolean" optional="yes" description="Should the forward be HTML?"/>
			<result type="specifier" description="to the resulting message or window"/>
		</command>
		<command name="redirect" code="MSOEmRdt" description="Redirect a message">
			<direct-parameter type="message" description="the message to redirect"/>
			<parameter name="to" code="insh" type="text" optional="yes" description="recipients of new message (as text)"/>
			<parameter name="opening window" code="OpWn" type="boolean" optional="yes" description="Should it create a window or a message? (Default is to create window)"/>
			<result type="specifier" description="to the resulting message or window"/>
		</command>
		<command name="retrieve free busy info for" code="MSOERFre" description="retrieve free busy info for given email addresses">
			<direct-parameter type="text" description="the email addresses of invitees"/>
			<parameter name="from" code="pFrm" type="date" description="date at which the calculation starts (only the date, no time)"/>
			<parameter name="to" code="insh" type="date" description="date at which the calculation ends (only the date, no time)"/>
			<result description="the free busy info for all invitees">
				<type type="text" list="yes"/>
			</result>
		</command>
		<command name="calculate recurring dates for" code="MSOECEve" description="calculate recurring dates for a recurring event">
			<direct-parameter type="event" description="the event to be calculated"/>
			<parameter name="from" code="pFrm" type="date" description="time(inclusive) at which the calculation starts"/>
			<parameter name="to" code="insh" type="date" description="time(inclusive) at which the calculation ends"/>
			<result description="the recurring dates">
				<type type="date" list="yes"/>
			</result>
		</command>
		<command name="reply to" code="MSOEmRpl" description="Reply to a message">
			<direct-parameter type="message" description="the message to reply to"/>
			<parameter name="opening window" code="OpWn" type="boolean" optional="yes" description="Should it create a window or a message? (Default is to create window)"/>
			<parameter name="attribution style" code="AtSt" type="atrE" optional="yes" description="attribution style to use"/>
			<parameter name="reply to all" code="rplA" type="boolean" optional="yes" description="Should the reply to go to all recipients of the message?"/>
			<parameter name="place insertion point on top" code="rplI" type="boolean" optional="yes" description="Should the insertion point be placed at the top the message? (only applies to internet-style attribution)"/>
			<parameter name="html text" code="AsHT" type="boolean" optional="yes" description="Should the reply be HTML?"/>
			<parameter name="quote entire message" code="QtMs" type="boolean" optional="yes" description="Should the entire message be quoted?"/>
			<result type="specifier" description="to the resulting message or window"/>
		</command>
		<command name="save calendar as web page" code="MSOESCal" description="Save Calendar as a web page">
			<parameter name="to" code="COut" type="file specification" optional="yes" description="web page destination"/>
			<parameter name="with title" code="CalT" type="text" optional="yes" description="title of the output web page"/>
			<parameter name="starting" code="CSta" type="date" optional="yes" description="the date the Calendar starts at"/>
			<parameter name="ending" code="CEnd" type="date" optional="yes" description="the date the Calendar ends at"/>
			<parameter name="including event details" code="CDet" type="boolean" optional="yes" description="include the event details?"/>
			<parameter name="using background graphics" code="CBGG" type="file specification" optional="yes" description="using background graphics"/>
		</command>
		<command name="resend" code="MSOEmRnd" description="Resend a message">
			<direct-parameter type="message" description="the message to resend"/>
			<parameter name="opening window" code="OpWn" type="boolean" optional="yes" description="Should it create a window or a message? (Default is to create window)"/>
			<result type="specifier" description="to the resulting message or window"/>
		</command>
		<command name="convert" code="MSOEAcnv" description="Convert between character sets">
			<direct-parameter type="any" description="the text to convert as a string or unicode text"/>
			<parameter name="from" code="pFrm" type="text" optional="yes" description="The name of the source character set, or leave out if source is Unicode."/>
			<parameter name="to" code="insh" type="text" optional="yes" description="The name of the destination character set, or leave out to return Unicode."/>
			<result type="any" description="the converted text as a string or unicode text"/>
		</command>
		<command name="link" code="MSOELink" description="Link an item to another item">
			<direct-parameter type="specifier" description="the item to link"/>
			<parameter name="to" code="insh" type="specifier" optional="yes" description="the item to link it to"/>
		</command>
		<command name="unlink" code="MSOEUnLk" description="Remove a link between two items">
			<direct-parameter type="specifier" description="the item to remove a link from"/>
			<parameter name="from" code="pFrm" type="specifier" optional="yes" description="the item to which it is currently linked"/>
		</command>
		<command name="import" code="MSOEImpt" description="Import data into Entourage">
			<direct-parameter type="any" description="the item to import"/>
			<parameter name="source" code="impt" type="ityp" optional="yes" description="type of import (default is MBOX)"/>
			<parameter name="to" code="insh" type="specifier" optional="yes" description="where to import the data (MBOX import only)"/>
			<parameter name="conflicts" code="cflx" type="cnfl" optional="yes" description="action to take for all archive records which conflict (default is skip)"/>
			<result type="specifier" description="for MBOX import, a folder reference is returned; for archive import, a bool which is true if there are conflicts"/>
		</command>
		<command name="export archive" code="MSOEXArc" description="export archive data">
			<parameter name="to" code="xdst" type="alias" description="destination file"/>
			<parameter name="only" code="xlim" type="specifier" optional="yes" description="limits export to the referenced category or project"/>
			<parameter name="item types" code="XITM" optional="yes" description="item types to archive (default is all items)">
				<type type="XAIt" list="yes"/>
			</parameter>
			<parameter name="delete" code="xdlt" type="boolean" optional="yes" description="delete records after exporting (default is false)"/>
			<parameter name="retain" code="xrtn" type="boolean" optional="yes" description="retain records that are also in another category or project (default is true; ignored if delete is false or if no category or project is specified)"/>
		</command>
		<command name="share" code="MSOEShPj" description="Share a project">
			<direct-parameter type="project" description="the project to share"/>
			<parameter name="location" code="prSD" type="file specification" description="path to file server location"/>
			<parameter name="current items" code="prSC" type="boolean" optional="yes" description="share items currently in project"/>
			<parameter name="added items" code="prSF" type="boolean" optional="yes" description="automatically share new items added to the project"/>
		</command>
		<command name="do LDAP search using account" code="MSOELSea" description="LDAP search using the specific account for a given name or email address">
			<direct-parameter type="LDAP server" description="the LDAP account"/>
			<parameter name="name" code="pnam" type="text" description="the name of the account"/>
			<parameter name="email address" code="cEmA" type="text" description="the e-mail address of the account"/>
			<result type="text" description="the info for given user or email address"/>
		</command>
		<command name="do OWA GAL search using account" code="MSOEOSea" description="Global Address List search using Outlook Web Access interface ">
			<direct-parameter type="Exchange account" description="the Exchange account"/>
			<parameter name="search string" code="SrSt" type="text" description="the search string to query the OWA GAL server with"/>
			<result description="a list of contact records returned by the OWA GAL server">
				<type type="record" list="yes"/>
			</result>
		</command>
		<command name="unshare" code="MSOEUSPj" description="Stop  sharing a project">
			<direct-parameter type="project" description="the project to unshare"/>
		</command>
		<command name="subscribe project" code="MSOESbPj" description="Subscribe to a project">
			<parameter name="to" code="Sfss" type="file specification" description="project file"/>
		</command>
		<command name="unsubscribe" code="MSOEUnsb" description="unsubscribe a folder">
			<direct-parameter type="specifier" description="the folder to unsubscribe --  must be other user's or public folder"/>
